{
    "name": "Gimloader",
    "description": "An extension that lets you use plugins to customize Gimkit",
    "version": "1.2.0",
    "manifest_version": 3,
    "key": "",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "images/icon128.png"
    },
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "content_scripts": [
        {
            "js": ["js/content/index.js"],
            "matches": [
                "https://www.gimkit.com/*"
            ],
            "run_at": "document_start",
            "world": "MAIN"
        }, {
            "js": ["js/installApi/index.js"],
            "matches": [
                "https://thelazysquid.github.io/Gimloader/*"
            ],
            "run_at": "document_start",
            "world": "MAIN"
        }
    ],
    "externally_connectable": {
        "matches": [
            "https://www.gimkit.com/*",
            "https://thelazysquid.github.io/Gimloader/*"
        ]
    },
    "background": {
        "service_worker": "js/background/index.js"
    },
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "edit_csp",
                "enabled": true,
                "path": "edit_csp.json"
            }
        ]
    },
    "permissions": [
        "storage",
        "unlimitedStorage",
        "declarativeNetRequest"
    ],
    "host_permissions": [
        "https://www.gimkit.com/*",
        "http://localhost:5822/*"
    ]
}